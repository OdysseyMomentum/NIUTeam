s: {\n            enabled: false\n         },\n         legend: {\n             display: false\n         },\n         title: {\n            display: true,\n            text: labelToDisplay\n        },\n        responsive: true,\n        maintainAspectRatio: false,\n        \n        scales: {\n          xAxes: [ {\n            type: 'time',\n             time: { displayFormats: { minute: \"HH:mm\" } },\n            display: true,\n            scaleLabel: {\n              display: true,\n            },\n            ticks: {\n                source: 'auto',\n            }\n          } ],\n          yAxes: [ {\n            display: true,\n            scaleLabel: {\n              display: true,\n            }\n          } ]\n        }\n      }\n    });\n  }\n\n  mounted() {\n     this.createChart('Temperature',this.$store.state.chartData.temperature);\n     console.log(this.$store.state.chartData.timestamp);\n     \n}\n\n}\n"],"sourceRoot":""}]}